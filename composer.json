  "type": "craft-plugin",
  "version": "1.0.3",
  "license": "mit",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.5.0",
    "datalinx/dpd-php-sdk": "*",
    "craftsnippets/craft-shipping-toolbox-base-plugin": "1.0.4"
  },
  "autoload": {
